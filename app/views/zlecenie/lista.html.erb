<table class="table-bordered table-striped table-hover align-middle">
  <thead>
    <tr>
      <th>Id kierownik</th>
      <th>Id opiekun</th>
      <th>Data utworzenia</th>
      <th>Data zlecenia</th>
      <th>Typ</th>
      <th>Opis</th>
      <th>Status</th>
      <th>Raport</th>
      <th>Info</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @zlecenie.each do |zlecenie| %>
      <tr>
        <td><%= zlecenie.kierownik_id %></td>
        <td><%= zlecenie.opiekun_id %></td>
        <td><%= zlecenie.data_utworzenia %></td>
        <td><%= zlecenie.data_zlecenia %></td>
        <td><%= zlecenie.typ_spotkania_id %></td>
        <td><%= zlecenie.opis %></td>
        <td><%= zlecenie.status_zlecenia_id %></td>
        <td><%= zlecenie.raport %></td>
        <td><%= zlecenie.info %></td>
        <td><%= link_to 'Show', zlecenie,  class: "btn btn-link"  %></td>
        </tr>
    <% end %>
  </tbody>
</table>

