<p id="notice"><%= notice %></p>

<h1>Listing Obroty</h1>

<table>
  <thead>
    <tr>
      <th>Id kontrahenci</th>
      <th>Rok</th>
      <th>Miesiac</th>
      <th>Kwota</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @obroty.each do |obroty| %>
      <tr>
        <td><%= obroty.Id_kontrahenci %></td>
        <td><%= obroty.Rok %></td>
        <td><%= obroty.Miesiac %></td>
        <td><%= obroty.Kwota %></td>
        <td><%= link_to 'Show', obroty %></td>
        <td><%= link_to 'Edit', edit_obroty_path(obroty) %></td>
        <td><%= link_to 'Destroy', obroty, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Obroty', new_obroty_path %>
